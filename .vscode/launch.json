{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Rhino 7 (Windows)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-plugin-net48",
            // Launches Rhino for us
            "program": "C:\\Program Files\\Rhino 7\\System\\Rhino.exe",
            // Add in any args here such as -nosplash or even -runscript
            // See : https://docs.mcneel.com/rhino/7/help/en-us/index.htm#information/startingrhino.htm
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            // RHINO_PACKAGE_DIRS is required for Multi-Targeted plugins
            // This is what enables Rhino to register our Plug-in
            "env": {
                "RHINO_PACKAGE_DIRS": "${workspaceFolder}/bin/Debug/"
            }
        }
    ],
    "compounds": []
}
